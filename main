#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);
    for (int i=2;i<n;i++)
    {
        if (n%i==0)
        {
            printf("%d", i);
            for (int j=2;j<i;j++)
            {
                if (i%j==0)
                {
                    printf(" %d", j);
                }
            }
            printf("\n");
        }
    }
    return 0;
}

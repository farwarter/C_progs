#include <stdio.h>

int main() {
    int n;
    int i, j;    

    scanf("%d", &n);
    
    for (i=0; i<n; i++)
    {
        for(j=0;j<n;j++)
        {
            if(j<(n-i)-1)
                write(1," ",1);
            else
                write(1,"*",1);
        }
        write(1,"\n",1);
    }
    return 0;
}
